<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.endof433">

    <application
        android:allowBackup="true"
        android:icon="@drawable/usatt"
        android:label="Ultimate Table Tennis"
        android:roundIcon="@drawable/usatt"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".NotificationActivity"></activity>
        <activity android:name=".RatingActivity"></activity>
        <activity android:name=".StatsActivity"></activity>
        <activity android:name=".FindClubsActivity"></activity>

        <receiver android:name=".AlertReceiver"/>
    </application>

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>

</manifest>